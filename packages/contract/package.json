    "@truffle/error": "^0.1.0",
    "@truffle/interface-adapter": "^0.5.16",
    "bignumber.js": "^7.2.1",
    "debug": "^4.3.1",
    "ethers": "^4.0.32",
    "web3": "1.5.3",
    "web3-core-helpers": "1.5.3",
    "web3-core-promievent": "1.5.3",
    "web3-eth-abi": "1.5.3",
