    "ethers": "^4.0.0-beta.1",
    "@truffle/interface-adapter": "^0.2.5",
    "web3": "1.2.1",
    "web3-core-promievent": "1.2.1",
    "web3-eth-abi": "1.2.1",
    "web3-utils": "1.2.1"
  },
  "devDependencies": {
    "async": "2.6.1",
    "browserify": "^14.0.0",
    "chai": "4.2.0",
