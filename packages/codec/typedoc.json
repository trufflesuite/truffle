{
  "name": "@truffle/codec",
  "mode": "modules",
  "module": "commonjs",
  "tsconfig": "tsconfig.json",
  "out": "dist/docs",
  "json": "dist/apidocs.json",
  "theme": "default",
  "ignoreCompilerErrors": false,
  "preserveConstEnums": true,
  "excludeExternals": true,
  "exclude": [
    "**/test/*"
  ],
  "target": "ES6"
}
