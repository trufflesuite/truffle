  },
  "version": "5.1.62",
  "main": "./build/library.bundled.js",
  "bin": {
    "truffle": "./build/cli.bundled.js"
  },
  "scripts": {
    "analyze": "./scripts/analyze.sh",
    "build": "yarn build-cli",
